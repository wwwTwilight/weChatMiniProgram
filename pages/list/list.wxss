/* pages/list/list.wxss */
.container1 view {
  width: 100px;
  height: 100px;
  text-align: center;
  line-height: 100px;
}
.container1 view:nth-child(1) {
  background-color: aqua;
}

.container1 view:nth-child(2) {
  background-color: pink;
}

.container1 view:nth-child(3) {
  background-color: red;
}

.container1
{
  display: flex;
  justify-content: space-around;
}

.goods-image
{
  width: 100%;
  margin: 10px;
  /* object-fit: cover; */

  background-color: aqua;
}